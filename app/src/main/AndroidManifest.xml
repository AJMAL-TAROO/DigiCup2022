<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freedomedition.zet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/zet_logo_whitebg"
        android:label="ZET"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/zet_logo_whitebg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.freedomedition.zet.pushDonation"></activity>
        <activity android:name="com.freedomedition.zet.pop_image_only" />
        <activity android:name="com.freedomedition.zet.ui.dashboard.DashboardFragment" />


        <activity
            android:name="com.freedomedition.zet.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>